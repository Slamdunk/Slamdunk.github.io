<?xml version="1.0"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
    <ShortName>Google (it)</ShortName>
    <Description>Google Search</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/png;base64,</Image>
    <Url type="application/x-suggestions+json" method="get" template="https://suggestqueries.google.com/complete/search?output=firefox&amp;client=firefox&amp;hl={moz:locale}&amp;q={searchTerms}"/>
    <Url type="text/html" method="get" template="https://www.google.it/search">
        <Param name="hl" value="it"/>
        <Param name="q" value="{searchTerms}"/>
    </Url>
</OpenSearchDescription>
